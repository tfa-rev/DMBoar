@{
    Layout = null;
    ViewBag.Title = "Register";
}
<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <title>Register</title>
    <link href="~/Content/custom-menus.css" rel="stylesheet" />
    <script src="~/scripts/jquery-3.3.1.min.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
    <script src="~/scripts/GetData.js"></script>
</head>

<body>

    <div class=" ">
        <div class=" ">
            <h> User info </h>
            <div class="stack-el">
                <div class="  stack-div   ">

                    <text> ID </text>
                    <input id="r-id" class="right-element" />

                </div>
                <div class=" stack-div   ">

                    <text> Username </text>
                    <input id="r-uname" class="right-element" />

                </div>
                <div class=" stack-div ">

                    <text> Mail </text>
                    <input id="r-mail" class="right-element" />
                </div>
                <div class="stack-div  ">

                    <text> First Name </text>
                    <input id="r-fname" class="right-element" />

                </div>
                <div class=" stack-div ">

                    <text> Last Name </text>
                    <input id="r-lname" class="right-element" />
                </div>
                <div class=" stack-div ">

                    <text> Password </text>
                    <input id="r-password" class="right-element" />
                </div>
                <div class="stack-div  ">

                    <text> Role </text>
                    <input id="r-role" class="right-element" />
                </div>
                <div class="stack-div  ">

                    <text> Location </text>
                    <input id="r-location" class="right-element" />
                </div>



            </div>

            <button id="btn-register" class=" classy "> Register </button>
            <button id="btn-update" class=" classy "> Update </button>

            <div>
                <label id="toast-register" class="log-item center-el">  </label>
            </div>

        </div>
    </div>

</body>

</html>



